{
  "compilerOptions": {
    "downlevelIteration": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "isolatedModules": false,
    "baseUrl": "./",
    "outDir": "./",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "chai",
      "mocha"
    ],
    "lib": [
      "es2016",
      "dom"
    ]
  },
  "include": [
    "*.ts",
  ],
  "exclude": []
}
